{
  "name": "calendar",
  "displayName": "Calendar",
  "description": "Interactive Calendar",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.108.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Calendar",
      "properties": {
        "calendar.dateFormat": {
          "type": "string",
          "default": "default",
          "description": "The format to use for the inserted date. Use 'default' for system locale format, or 'ISO' for YYYY-MM-DD."
        }
      }
    },
    "commands": [
      {
        "command": "calendar.insertDate",
        "title": "Insert Date from Calendar"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.108.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.54.0",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
